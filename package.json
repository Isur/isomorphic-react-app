{
  "name": "isomorphic-react-app",
  "version": "1.0.0",
  "description": "Node, React, Typescript, Babel, Webpack, Eslint, SASS, Jest",
  "repository": "https://github.com/Isur/isomorphic-react-app",
  "author": "Isur",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test_watch": "jest --watch --coverage",
    "precommit": "npm run linter-no-warning && npm run typecheck && npm run test && npm outdated",
    "predev": "npm run linter-fix & rm -rf dist/",
    "dev": "webpack --config webpack/server.dev.babel.js",
    "linter": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-path .gitignore ./src",
    "typecheck": "tsc --noEmit",
    "linter-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --color --ignore-path .gitignore ./src --fix",
    "linter-no-warning": "eslint --max-warnings 0 --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-path .gitignore ./src",
    "prebuild": "npm run precommit",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "rm -rf build/",
    "build:server": "webpack --config ./webpack/server.babel.js",
    "build:client": "webpack --config ./webpack/client.babel.js",
    "dockerbuild": "npm run build:server &&  npm run build:client",
    "start": "cd build && node ./server.js"
  },
  "keywords": [
    "webpack",
    "typescript",
    "node",
    "babel",
    "sass",
    "eslint",
    "jest"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack": "^4.41.7",
    "@types/webpack-dev-middleware": "^3.7.0",
    "@types/webpack-hot-middleware": "^2.25.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.1.0",
    "husky": "^4.2.3",
    "ignore-loader": "^0.1.2",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "react-hot-loader": "^4.12.19",
    "react-test-renderer": "^16.13.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
